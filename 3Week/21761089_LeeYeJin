            console.log("일치합니다."); 
            }
        else {
            console.log("일치하지 않습니다.");
        }


        //Quiz 02.
        var _Quiz =['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','v','w','u','x','y','z'];
        for (var i =0; i< _Quiz.length; i++)
        {
            console.log(_Quiz[i]);//따로 출력하게 함.
            }

        var d =''
        for( var i =0; i < _Quiz.length; i++){
            if( i>0 ){
                d += ' ';
            }
            d += _Quiz[i];

             
        }
         {  console.log(d); }  //한번에 출력
             


         //Quiz 03.

         for(var i=1; i<=50; i++){
            if(i%2==1){ 
                console.log(i+"= 홀"); 
            }
            else{
                console.log(i+"= 짝"); 
            }

         }
        

        //답은 맞게 나왔는데..올바르게 작성했는지는 잘 모르겠습니다!ㅜㅜ
        //넘 헷갈립니다...더 열시미 하겠습니다 !^______^*

